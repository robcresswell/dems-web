{
  "name": "{{ name }}",
  "main": "dist/main.js",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "NODE_ENV=development tsc-watch --onSuccess 'node --unhandled-rejections=strict .'",
    "lint": "prettier --check '{src,test}/**/*.ts' && eslint '{src,test}/**/*.ts'",
    "lint:fix": "prettier --write {src,test}/**/*.ts && eslint --fix '{src,test}/**/*.ts'",
    "start": "NODE_ENV=production node --unhandled-rejections=strict .",
    "test": "jest --ci",
    "test:coverage": "jest --ci --coverage"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^2.13.0",
    "fastify-swagger": "^2.5.0"
  },
  "devDependencies": {
    "@types/eslint": "6.1.9",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.34",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "get-port": "^5.1.1",
    "jest": "^25.2.4",
    "jest-junit": "^10.0.0",
    "pino-pretty": "^3.6.1",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
