{
  "name": "{{ name }}",
  "main": "dist/main.js",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "dev": "NODE_ENV=development tsc-watch --onSuccess 'node --enable-source-maps -r dotenv/config .'",
    "lint": "run-p --aggregate-output lint:eslint lint:prettier",
    "lint:prettier": "prettier --check '{src,test}/**/*.{ts,json,md}' --loglevel 'warn'",
    "lint:eslint": "eslint --color '{src,test}/**/*.ts'",
    "lint:fix": "prettier --write '{src,test}/**/*.{ts,json,md}' --loglevel 'warn' && eslint --fix '{src,test}/**/*.ts'",
    "start": "NODE_ENV=production node --enable-source-maps -r dotenv/config .",
    "test": "jest --ci",
    "test:generated-types": "./scripts/generate-types.ts --check-only",
    "test:coverage": "jest --ci --coverage ; open-cli 'reports/coverage/index.html'"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-swagger": "^5.0.0",
    "got": "^11.8.5",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "get-port": "^5.1.1",
    "globby": "^11.1.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "pino-pretty": "^7.5.4",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.2"
  }
}
